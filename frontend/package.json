{
  "name": "react-authentication",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest",
    "preview": "vite preview",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings=0 --cache",
    "lint-fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "lint-styling": "npx stylelint \"**/*.{css,scss}\" --max-warnings=0",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "generate-types": "orval --config orval.config.ts"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1",
    "zod": "^3.25.67",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "8.57.0",
    "eslint-plugin-css-modules": "2.12.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-sonarjs": "1.0.3",
    "eslint-plugin-unicorn": "51.0.1",
    "globals": "^15.14.0",
    "husky": "^9.0.11",
    "jsdom": "^26.0.0",
    "jwt-decode": "^4.0.0",
    "orval": "6.29.1",
    "sass": "^1.83.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-declaration-strict-value": "1.9.2",
    "stylelint-order": "^6.0.4",
    "stylelint-snapshot-14.16.1": "npm:stylelint@14.16.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6"
